{
  "name": "leavify",
  "type": "module",
  "version": "0.2.1",
  "description": "A package to find object leaf values, compare changes of leaves by path, and reconstruct the 'tree' object from its leaves",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.195",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "dev": "npx tsx src/index.ts",
    "test": "vitest",
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Davi Alexandre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davi-alexandre/leavify.git"
  },
  "bugs": {
    "url": "https://github.com/davi-alexandre/leavify/issues"
  },
  "homepage": "https://github.com/davi-alexandre/leavify#readme",
  "keywords": [
    "object",
    "leaves",
    "leaf",
    "path",
    "value",
    "path-value",
    "change",
    "difference",
    "tree"
  ]
}
